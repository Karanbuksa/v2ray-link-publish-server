{
  "name": "v2ray-link-publish-server",
  "version": "1.0.0",
  "description": "Server for publishing personalized V2Ray links from 3x-ui panel",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "start": "node dist/index.js",
    "migrate": "ts-node src/database/migrations/run.ts"
  },
  "keywords": ["v2ray", "3x-ui", "proxy"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.2",
    "http-cookie-agent": "^6.0.5",
    "tough-cookie": "^4.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "better-sqlite3": "^9.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/uuid": "^9.0.7",
    "@types/tough-cookie": "^4.0.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
